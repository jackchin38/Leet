class Solution:
    def isPalindrome(self, x):
        a = str(x)
        if len(a) != 2 or len(a) != 2 or len(a) != 6 or len(a) != 8:
            b = a[::-1]
            if a == b:
                return(True)
            else:
                return(False)
        else:
            return(false)
        
